{
  "name": "bit-docs-website",
  "version": "0.0.1",
  "description": "Website that documents bit-docs",
  "keywords": [
    "documentation",
    "donejs",
    "website"
  ],
  "homepage": "https://github.com/bit-docs/website#readme",
  "bugs": {
    "url": "https://github.com/bit-docs/website/issues"
  },
  "license": "MIT",
  "author": "Bitovi",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bit-docs/website.git"
  },
  "scripts": {
    "bit-docs": "bit-docs",
    "gen": "bit-docs -d",
    "genc": "bit-docs -dc",
    "genf": "bit-docs -df",
    "gh-pages": "gh-pages -d gh-pages",
    "pub": "npm run gh-pages",
    "see": "http-server gh-pages",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "bit-docs": "bit-docs/bit-docs",
    "bit-docs-dev": "bit-docs/bit-docs-dev",
    "bit-docs-generate-html": "bit-docs/bit-docs-generate-html",
    "bit-docs-generate-readme": "bit-docs/bit-docs-generate-readme",
    "bit-docs-glob-finder": "bit-docs/bit-docs-glob-finder",
    "bit-docs-js": "bit-docs/bit-docs-js",
    "bit-docs-process-mustache": "bit-docs/bit-docs-process-mustache",
    "bit-docs-process-tags": "bit-docs/bit-docs-process-tags",
    "bit-docs-tag-sourceref": "bit-docs/bit-docs-tag-sourceref",
    "bit-docs-type-annotate": "bit-docs/bit-docs-type-annotate",
    "gh-pages": "^0.12.0"
  },
  "bit-docs": {
    "dependencies": {
      "bit-docs-dev": "bit-docs/bit-docs-dev",
      "bit-docs-generate-html": "bit-docs/bit-docs-generate-html",
      "bit-docs-glob-finder": "bit-docs/bit-docs-glob-finder",
      "bit-docs-js": "bit-docs/bit-docs-js#remove-depend"
    },
    "glob": {
      "pattern": "{node_modules,docs}/{bit-docs,bit-docs-dev,bit-docs-generate-html,bit-docs-generate-readme,bit-docs-glob-finder,bit-docs-js,bit-docs-process-mustache,bit-docs-process-tags,bit-docs-tag-sourceref,bit-docs-type-annotate}/**/*.{js,md}",
      "ignore": [
        "node_modules/bit-docs/README.md",
        "node_modules/bit-docs/license.md",
        "node_modules/bit-docs/lib/configure/**/*",
        "node_modules/bit-docs*/node_modules/**/*",
        "node_modules/bit-docs*/**/test/**/*",
        "node_modules/bit-docs*/**/*_test.js",
        "node_modules/bit-docs*/**/test_*.js",
        "node_modules/bit-docs*/**/test.js",
        "node_modules/bit-docs-generate-html/site/**/*"
      ]
    },
    "parent": "BitDocs",
    "minifyBuild": false,
    "dest": "./gh-pages"
  }
}
